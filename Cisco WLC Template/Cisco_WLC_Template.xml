            <discovery_rules>
                <discovery_rule>
                    <uuid>2b3e8d887d3942db88e2b4f32b2a1180</uuid>
                    <name>AP Discovery</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>walk[.1.3.6.1.4.1.14179.2.2.1.1.3,.1.3.6.1.4.1.14179.2.2.1.1.30]</snmp_oid>
                    <key>bsnAPName</key>
                    <delay>1h</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#AP_NAME}</macro>
                                <value>^\w{2}-.+$</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <lifetime>30d</lifetime>
                    <enabled_lifetime_type>DISABLE_NEVER</enabled_lifetime_type>
                    <description>AIRESPACE-WIRELESS-MIB::bsnAPName
AIRESPACE-WIRELESS-MIB::bsnAPGroupVlanName</description>
                    <host_prototypes>
                        <host_prototype>
                            <uuid>ee624a6f48df43c199e3b6d147bf3b83</uuid>
                            <host>{#AP_NAME}</host>
                            <name>{#AP_NAME}</name>
                            <group_links>
                                <group_link>
                                    <group>
                                        <name>Network devices</name>
                                    </group>
                                </group_link>
                            </group_links>
                            <templates>
                                <template>
                                    <name>Template Cisco LAP</name>
                                </template>
                            </templates>
                            <tags>
                                <tag>
                                    <tag>class</tag>
                                    <value>WiFi</value>
                                </tag>
                                <tag>
                                    <tag>flexgroup</tag>
                                    <value>{#AP_GROUP}</value>
                                </tag>
                                <tag>
                                    <tag>role</tag>
                                    <value>AP</value>
                                </tag>
                                <tag>
                                    <tag>vendor</tag>
                                    <value>cisco</value>
                                </tag>
                            </tags>
                        </host_prototype>
                    </host_prototypes>
                    <preprocessing>
                        <step>
                            <type>SNMP_WALK_TO_JSON</type>
                            <parameters>
                                <parameter>{#AP_NAME}</parameter>
                                <parameter>.1.3.6.1.4.1.14179.2.2.1.1.3</parameter>
                                <parameter>0</parameter>
                                <parameter>{#AP_GROUP}</parameter>
                                <parameter>.1.3.6.1.4.1.14179.2.2.1.1.30</parameter>
                                <parameter>0</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                </discovery_rule>
            </discovery_rules>
            <tags>
                <tag>
                    <tag>class</tag>
                    <value>WiFi</value>
                </tag>
                <tag>
                    <tag>devicegroup</tag>
                    <value>network</value>
                </tag>
                <tag>
                    <tag>role</tag>
                    <value>WLC</value>
                </tag>
                <tag>
                    <tag>vendor</tag>
